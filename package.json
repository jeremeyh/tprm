{
  "name": "slack-dm-redirect-bot",
  "version": "2.1.0",
  "description": "Slack DM Redirect Bot (Express + Bolt) with OAuth install routes for Vercel",
  "private": true,
  "type": "commonjs",
  "main": "app.js",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "nodemon --watch . --ext js,json --exec node app.js",
    "start": "node app.js"
  },
  "dependencies": {
    "@slack/bolt": "^3.17.1",
    "@slack/web-api": "^6.12.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "body-parser": "^1.20.3"
  },
  "devDependencies": {
    "nodemon": "^3.1.7"
  },
  "overrides": {
    "semver": "^7.6.3",
    "@vercel/remix-builder>semver": "^7.6.3",
    "@vercel/redwood>semver": "^7.6.3",
    "degenerator": "^5.0.1"
  }
}
